{
  "name": "rockbuzz/lara-uuid",
  "description": "Laravel Uuid",
  "keywords": ["laravel", "uuid"],
  "license": "MIT",
  "authors": [
    {
      "name": "Tiago Lopes",
      "email": "tiago@rockbuzz.com.br"
    },
    {
      "name": "Paulo Henrique P. Scheuermann",
      "email": "paulo@rockbuzz.com.br"
    }
  ],
  "autoload": {
    "psr-4": {
      "Rockbuzz\\LaraUuid\\": "src/"
    }
  },
  "autoload-dev": {
